# 2 bytes
# byte1 tempthresh
# byte2 period(msb 4 bits) | hysterisis(lsb 4 bits)
# 
HNBU_TEMPTHRESH {
	cis_tuple	CISTPL_BRCM_HNBU,HNBU_TEMPTHRESH
	compat		>= 0
	u8 tempthresh {
		cis	{ 0x0 }
		srom 8-10	{ 0xB2 }
		srom >= 11	{ 0xAE }
	}
	u8 temps_period {
		sfmt	sdec
		cis	{ 0x1 (&0xF0, >>4) }
		srom 8-10	{ 0xBC (&0xF) }
		srom >= 11	{ 0xB8 (&0xF) }
	}
	u8 temps_hysteresis {
		sfmt	sdec
		cis	{ 0x1 (&0xF) }
		srom 8-10	{ 0xBC (&0xF0, >>4) }
		srom >= 11	{ 0xB8 (&0xF0, >>4) }
	}
	u8 tempoffset {
		sfmt	sdec
		cis	{ 0x3 }
		srom 8-10	{ 0xB3 }
		srom >= 11	{ 0xAF }
	}
	u8 tempsense_slope {
		cis	{ 0x4 }
		srom 8-10	{ 0xB7 }
		srom >= 11	{ 0xB3 }
	}
	u8 tempcorrx {
		cis	{ 0x4 (&0xFC, >>2) }
		srom 8-10	{ 0xB6 (&0xFC, >>2) }
		srom >= 11	{ 0xB2 (&0xFC, >>2) }
	}
	u8 tempsense_option {
		cis	{ 0x4 (&0x3) }
		srom 8-10	{ 0xB6 (&0x3) }
		srom >= 11	{ 0xB2 (&0x3) }
	}
	u8 phycal_tempdelta {
		sfmt	sdec
		cis	{ 0x7 }
		srom 8-10	{ 0xBD }
		srom >= 11	{ 0xB9 }
	}
}

