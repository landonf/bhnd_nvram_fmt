		cis	{ 0x3 }
		srom 	{  u8 @ 0x1BF (&0x1F),  u8 @ 0x1C1 (&0x1F),  u8 @ 0x1C3 (&0x1F),  u8 @ 0x1C5 (&0x1F) }
	}
	u16[4] noiselvl5ga1 {
		sfmt	sdec
		cis	{ u8[4] @ 0x7 }
		srom 	{ 0x1BE (&0x3E0, >>5) }
	}
	u8[4] noiselvl5ga2 {
		sfmt	sdec
