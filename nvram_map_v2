		srom 8-10	{ 0xA3 (&0xF) }
		srom >= 11	{ 0xA9 (&0xF) }
	}
	u8 rxchain {
		cis	{ 0x1 }
		srom 4-7	{ 0x7B (&0xF0, >>4) }
		srom 8-10	{ 0xA3 (&0xF0, >>4) }
		srom >= 11	{ 0xA9 (&0xF0, >>4) }
	}
