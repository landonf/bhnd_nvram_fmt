		srom 	{  u8 @ 0x1BE (&0x7C, >>2),  u8 @ 0x1C0 (&0x7C, >>2),  u8 @ 0x1C2 (&0x7C, >>2),  u8 @ 0x1C4 (&0x7C, >>2) }
	}
