		cis	{ 0x0 }
		srom 1	{  u16 @ 0x72 }
		srom 2	{  u16 @ 0x72 |  u16 @ 0x38 (<<16) }
		srom 3	{  u16 @ 0x72 |  u16 @ 0x7A (<<16) }
		srom 4	{ 0x44 }
		srom 5-7	{ 0x4A }
		srom >= 8	{ 0x84 }
	}
	u32 boardflags2 {
		cis	{ 0x4 }
		srom 4	{ 0x48 }
		srom 5-7	{ 0x4E }
		srom >= 8	{ 0x88 }
	}
	u32 boardflags3 {
		cis	{ 0x8 }
		srom >= 11	{ 0x8C }
	}
}

