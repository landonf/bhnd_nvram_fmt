# 1 byte bit 7 initial polarity, bit 6..0 gpio pin
HNBU_WOWLGPIO {
	cis_tuple	CISTPL_BRCM_HNBU,HNBU_WOWLGPIO
	compat		>= 0
	u8 wowl_gpio {
		sfmt	sdec
		cis	{ 0x0 (&0x7F) }
	}
