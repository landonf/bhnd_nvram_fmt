# SROM variables not supported by CIS
srom_misc {
	u16 cckPwrOffset {
		srom 10	{ 0x1B4 }
	}
	u8 et1macaddr {
		sfmt	macaddr
		srom 0-2	{  u8[48] @ 0x54 (&0xFFFF) }
	}
	u8 eu_edthresh2g {
		srom 8	{ 0x1A9 }
		srom 9	{ 0x199 }
		srom 10	{ 0x199 }
		srom 11	{ 0x1D1 }
	}
	u8 eu_edthresh5g {
		srom 8	{ 0x1A8 }
		srom 9	{ 0x198 }
		srom 10	{ 0x198 }
		srom 11	{ 0x1D0 }
	}
	u8 freqoffset_corr {
		srom 8-10	{ 0xB9 (&0xF) }
	}
	u8 hw_iqcal_en {
		srom 8-10	{ 0xB9 (&0x20, >>5) }
	}
	u8 il0macaddr {
		sfmt	macaddr
		srom 0-2	{  u8[48] @ 0x48 (&0xFFFF) }
	}
	u8 iqcal_swp_dis {
		srom 8-10	{ 0xB9 (&0x10, >>4) }
	}
	u8 noisecaloffset {
		srom 8-9	{ 0x1B5 }
	}
	u8 noisecaloffset5g {
		srom 8-9	{ 0x1B4 }
	}
	u8 noiselvl5gha0 {
		srom 8-10	{ 0x1B1 (&0x1F) }
	}
	u16 noiselvl5gha1 {
		srom 8-10	{ 0x1B0 (&0x3E0, >>5) }
	}
	u8 noiselvl5gha2 {
		srom 8-10	{ 0x1B0 (&0x7C, >>2) }
	}
	u8 noiselvl5gla0 {
		srom 8-10	{ 0x1AD (&0x1F) }
	}
	u16 noiselvl5gla1 {
		srom 8-10	{ 0x1AC (&0x3E0, >>5) }
	}
	u8 noiselvl5gla2 {
		srom 8-10	{ 0x1AC (&0x7C, >>2) }
	}
	u8 noiselvl5gma0 {
		srom 8-10	{ 0x1AF (&0x1F) }
	}
	u16 noiselvl5gma1 {
