HNBU_RXGAIN_ERR {
	cis_tuple	CISTPL_BRCM_HNBU,HNBU_RXGAIN_ERR
	compat		>= 11
	u8 rxgainerr2ga0 {
		cis	{ 0x0 }
		srom 8-10	{ 0x19B (&0x3F) }
		srom >= 11	{ 0x1C7 (&0x3F) }
	}
	u16 rxgainerr2ga1 {
		cis	{ u8 @ 0x1 }
		srom 8-10	{ 0x19A (&0x7C0, >>6) }
		srom >= 11	{ 0x1C6 (&0x7C0, >>6) }
	}
	u8 rxgainerr2ga2 {
		cis	{ 0x2 }
		srom 8-10	{ 0x19A (&0xF8, >>3) }
		srom >= 11	{ 0x1C6 (&0xF8, >>3) }
	}
	u8[4] rxgainerr5ga0 {
		cis	{ 0x3 }
		srom 	{  u8 @ 0x1C9 (&0x3F),  u8 @ 0x1CB (&0x3F),  u8 @ 0x1CD (&0x3F),  u8 @ 0x1CF (&0x3F) }
	}
	u16[4] rxgainerr5ga1 {
		cis	{ u8[4] @ 0x7 }
		srom 	{ 0x1C8 (&0x7C0, >>6) }
	}
	u8[4] rxgainerr5ga2 {
		cis	{ 0xB }
		srom 	{  u8 @ 0x1C8 (&0xF8, >>3),  u8 @ 0x1CA (&0xF8, >>3),  u8 @ 0x1CC (&0xF8, >>3),  u8 @ 0x1CE (&0xF8, >>3) }
	}
	u8 rxgainerr5gha0 {
		srom 8-10	{ 0x1A1 (&0x3F) }
	}
	u16 rxgainerr5gha1 {
		srom 8-10	{ 0x1A0 (&0x7C0, >>6) }
	}
	u8 rxgainerr5gha2 {
		srom 8-10	{ 0x1A0 (&0xF8, >>3) }
	}
	u8 rxgainerr5gla0 {
		srom 8-10	{ 0x19D (&0x3F) }
	}
	u16 rxgainerr5gla1 {
		srom 8-10	{ 0x19C (&0x7C0, >>6) }
	}
	u8 rxgainerr5gla2 {
		srom 8-10	{ 0x19C (&0xF8, >>3) }
	}
	u8 rxgainerr5gma0 {
		srom 8-10	{ 0x19F (&0x3F) }
	}
	u16 rxgainerr5gma1 {
		srom 8-10	{ 0x19E (&0x7C0, >>6) }
	}
	u8 rxgainerr5gma2 {
		srom 8-10	{ 0x19E (&0xF8, >>3) }
	}
	u8 rxgainerr5gua0 {
		srom 8-10	{ 0x1A3 (&0x3F) }
	}
	u16 rxgainerr5gua1 {
