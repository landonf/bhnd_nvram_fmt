		cis	{ 0x0 }
		srom 9-10	{ 0x14C }
	}
	u32 legofdmbw20ul5glpo {
		cis	{ 0x4 }
		srom 9-10	{ 0x150 }
	}
	u32 legofdmbw205gmpo {
		cis	{ 0x8 }
		srom 9-10	{ 0x154 }
	}
	u32 legofdmbw20ul5gmpo {
		cis	{ 0xC }
		srom 9-10	{ 0x158 }
	}
	u32 legofdmbw205ghpo {
		cis	{ 0x10 }
		srom 9-10	{ 0x15C }
	}
	u32 legofdmbw20ul5ghpo {
		cis	{ 0x14 }
		srom 9-10	{ 0x160 }
	}
}

