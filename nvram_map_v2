# 2 bytes; LED duty cycle
HNBU_LEDDC {
	cis_tuple	CISTPL_BRCM_HNBU,HNBU_LEDDC
	compat		>= 0
	u16 leddc {
		sfmt	leddc
		cis	{ 0x0 }
		srom 3	{  u8[2] @ 0x7C (&0xFFFF) }
		srom 4	{  u8[2] @ 0x5A (&0xFFFF) }
		srom 5-7	{  u8[2] @ 0x5A (&0xFFFF) }
		srom 8-10	{  u8[2] @ 0x9A (&0xFFFF) }
		srom >= 11	{  u8[2] @ 0x9E (&0xFFFF) }
	}
}

