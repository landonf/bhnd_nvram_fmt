		cis	{ 0x6 }
		srom 	{ 0xD0 }
	}
	u16 pdoffset80ma1 {
		cis	{ 0x8 }
		srom 	{ 0xD2 }
	}
	u16 pdoffset80ma2 {
		cis	{ 0xA }
		srom 	{ 0xD4 }
	}
}

# Two 16bit values: PCI vendor & device id
HNBU_CHIPID {
	cis_tuple	CISTPL_BRCM_HNBU,HNBU_CHIPID
	compat		>= 0
	u16 vendid {
		cis	{ 0x0 }
	}
	u16 devid {
		cis	{ 0x2 }
		srom >= 8	{ 0x60 }
	}
	u16 chiprev {
		sfmt	sdec
		cis	{ 0x4 }
	}
	u16 subvendid {
		cis	{ 0x6 }
	}
	u16 subdevid {
		cis	{ 0x8 }
	}
}

