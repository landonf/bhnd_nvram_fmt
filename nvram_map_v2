# Country code (2 bytes ascii + 1 byte cctl)
# in rev 2
# 
HNBU_CCODE {
	cis_tuple	CISTPL_BRCM_HNBU,HNBU_CCODE
	compat		>= 0
	char ccode {
		sfmt	ccode
		cis	{ u16 @ 0x0 }
		srom 0-3	{  char[2] @ 0x76 (&0xFFFF) }
		srom 4	{  char[2] @ 0x52 (&0xFFFF) }
		srom 5-7	{  char[2] @ 0x44 (&0xFFFF) }
		srom 8-10	{  char[2] @ 0x92 (&0xFFFF) }
		srom >= 11	{  char[2] @ 0x96 (&0xFFFF) }
	}
	u8 cctl {
		cis	{ 0x2 }
	}
}

